                    android:layout_height="wrap_content"
                    android:text="0"
                    android:gravity="center"
                    android:fontFamily="@font/josefinsans_bold"
                    android:layout_centerHorizontal="true"
                    android:textSize="8sp"
                    android:textColor="@color/white"
